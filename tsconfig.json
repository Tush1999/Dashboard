{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "moduleResolution":"node",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "jsx": "react-jsx",
      "sourceMap": true,
      "strict": true,
      "noEmit": true,
      "allowImportingTsExtensions": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
    },
    "include": ["src"]
  }
  